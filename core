vim9script
if !exists("g:mapleader")
	g:mapleader = ' '
        noremap  <C-j> <C-w>j
        noremap <C-k> <C-w>k
        noremap <C-h> <C-w>h
        noremap <C-l> <C-w>l
        noremap <C-K> <C-w>K
        noremap <C-H> <C-w>H
        noremap <C-J> <C-w>J
        noremap <C-L> <C-w>L

        noremap <Tab>w <cmd>tabnew<cr>
        noremap <Tab>h <cmd>tabnext<cr>
        noremap <Tab>l <cmd>tabproview<cr>
        noremap <Tab>q <cmd>tabclose<cr>

	nnoremap <leader>fl <cmd>Leaderf line <cr>
	nnoremap <leader>fm <cmd>Leaderf! mru<cr>
	nnoremap <leader>fb <cmd>Leaderf! buffer <cr>
	nnoremap <leader>F <cmd>Leaderf function<cr>
	nnoremap <leader>ff <cmd>Leaderf file<cr>
	nnoremap <leader>ft <cmd>Leaderf tag<cr>


endif
